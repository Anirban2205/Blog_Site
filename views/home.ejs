<%- include("partials/header") -%>

<body>
  <div class="container">
    <h1>Home</h1>
    <p><%= initial_content %></p>

    <% blogs.forEach(function(post){ %>
    <h2><%= post.title %></h2>
    <p>
      <%= post.content.substring(0,100) + " ..." %>
      <a href="/posts/<%= post.title %>">Read More</a>
    </p>
    <% }); %>
  </div>
</body>

<%- include("partials/footer") -%>
